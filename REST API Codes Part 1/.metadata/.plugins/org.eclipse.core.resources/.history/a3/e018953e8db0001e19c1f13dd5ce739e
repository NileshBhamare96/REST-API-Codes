package in.nileshit.bindings;

import jakarta.validation.constraints.Min;
import jakarta.validation.constraints.NotNull;
import jakarta.validation.constraints.Size;
import lombok.Data;

@Data
public class Person {

	@NotNull
	@Size(min = 3, max = 8)
	private String name;

	@NotNull
	@Min(18)
	private Integer age;

}